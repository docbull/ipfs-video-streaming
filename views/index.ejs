    <script>
      function mainButton() {
        window.location.href = '/';
      }
      function playVideo(cid) {
        window.location.href = '/player?cid=' + cid;
      }
      async function connectWalletPressed() {
        if (window.ethereum) {
          try {
            const addressArray = await window.ethereum.request({
              method: "eth_requestAccounts",
            });
            if (addressArray.length > 0) {
              console.log("ü¶ä MetaMask connected!");
              document.getElementById('walletButton').src = "/MetaMask_Logged_In.png";
            } else {
              console.log("ü¶ä Please connect to MetaMask using the top-right button.");
            }
          } catch (error) {
            console.log("üò• Something went wrong." + error);
          }
        }
      }
    </script>

    <script>
      document.addEventListener("DOMContentLoaded", async () => {
        if (window.ethereum) {
          try {
            const addressArray = await window.ethereum.request({
              method: "eth_accounts",
            });
            if (addressArray.length > 0) {
              console.log("ü¶ä MetaMask connected!");
              document.getElementById('walletButton').src = "/MetaMask_Logged_In.png";
            } else {
              console.log("ü¶ä Please connect to MetaMask using the top-right button.");
              document.getElementById('walletButton').src = "/MetaMask_Logged_Out.png";
            }
          } catch (error) {
            console.log("üò¢ " + error);
          }
        } else {
          console.log("You need to install MetaMask");
        }
      });
    </script>
